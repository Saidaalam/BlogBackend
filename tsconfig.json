{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "target": "ES6",
    "moduleResolution": "node",
    "module": "commonjs"
  },
  "include": [
    "src/**/*.ts",
    "src/@types/**/*.d.ts" 
, "src/utils.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
